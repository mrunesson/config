Host *.github.com
	IdentityFile ~/.ssh/github_rsa
    AddKeysToAgent yes

Host *.local
	ControlPath ~/.ssh/sock/%r@%h:%p 
	ControlMaster auto 
	Protocol 2
	NoHostAuthenticationForLocalhost yes
	CheckHostIP no
    AddKeysToAgent yes

Host 192.168.122.?
	ControlPath ~/.ssh/sock/%r@%h:%p 
	ControlMaster auto 
	Protocol 2
	NoHostAuthenticationForLocalhost yes
	CheckHostIP no
    AddKeysToAgent yes


Host *
	ControlPath ~/.ssh/sock/%r@%h:%p 
	ControlMaster auto 
	Protocol 2
	NoHostAuthenticationForLocalhost yes
    AddKeysToAgent yes